        /* Client-specific Styles */
    #outlook a {
        padding: 0;
    }

        /* Get rid of Outlook 2007-10 3px spacing left and right of table with an align attribute.  */
    table {
        border-collapse: collapse;
        mso-table-lspace: 0pt;
        mso-table-rspace: 0pt;
    }

        /* Force Outlook to provide a "view in browser" button. */
    body {
        width: 100% !important;
        color: #e2e2e2;
    }

    .ReadMsgBody {
        width: 100%;
    }

    .ExternalClass {
        width: 100%;
        display: block !important;
    }

        /* Force Hotmail to display emails at full width */
        /* Reset Styles */
        /* Add 100px so mobile switch bar doesn't cover street address. */
    html, body {
        background-color: #ececec;
        margin: 0;
        padding: 0;
    }

    img {
        height: auto;
        line-height: 100%;
        outline: none;
        text-decoration: none;
        display: block;
    }

    br, strong br, b br, em br, i br {
        line-height: 100%;
    }

    h1, h2, h3, h4, h5, h6 {
        line-height: 100% !important;
        -webkit-font-smoothing: antialiased;
    }

    h1 a, h2 a, h3 a, h4 a, h5 a, h6 a {
        color: white !important;
    }

    h1 a:active, h2 a:active, h3 a:active, h4 a:active, h5 a:active, h6 a:active {
        color: white !important;
    }

        /* Preferably not the same color as the normal header link color.  There is limited support for psuedo classes in email clients, this was added just for good measure. */
    h1 a:visited, h2 a:visited, h3 a:visited, h4 a:visited, h5 a:visited, h6 a:visited {
        color: white !important;
    }

        /* Preferably not the same color as the normal header link color. There is limited support for psuedo classes in email clients, this was added just for good measure. */
    table td, table tr {
        border-collapse: collapse;
    }

    .yshortcuts, .yshortcuts a, .yshortcuts a:link, .yshortcuts a:visited, .yshortcuts a:hover, .yshortcuts a span {
        color: black;
        text-decoration: none !important;
        border-bottom: none !important;
        background: none !important;
    }

        /* Body text color for the New Yahoo.  This example sets the font of Yahoo's Shortcuts to black. */
        /* This most probably won't work in all email clients. Don't include <code _tmplitem="502" > blocks in email. */
    code {
        white-space: normal;
        word-break: break-all;
    }

    #background-table {
        background-color: #ececec;
    }

        /* Webkit Elements */
    #top-bar {
        border-radius: 6px 6px 0px 0px;
        -moz-border-radius: 6px 6px 0px 0px;
        -webkit-border-radius: 6px 6px 0px 0px;
        -webkit-font-smoothing: antialiased;
        background-color: #555555;
        color: #e2e2e2;
    }

    #top-bar a {
        font-weight: bold;
        color: #e2e2e2;
        text-decoration: none;
    }

    #footer {
        border-radius: 0px 0px 6px 6px;
        -moz-border-radius: 0px 0px 6px 6px;
        -webkit-border-radius: 0px 0px 6px 6px;
        -webkit-font-smoothing: antialiased;
    }

        /* Fonts and Content */
    body {
        font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
    }

    .header-content, .footer-content-left, .footer-content-right {
        -webkit-text-size-adjust: none;
        -ms-text-size-adjust: none;
    }

        /* Prevent Webkit and Windows Mobile platforms from changing default font sizes on header and footer. */
    .header-content {
        font-size: 12px;
        color: <%= @studio.minisite.font_color -%>;
    }

    .header-content a {
        font-weight: bold;
        color: <%= @studio.minisite.font_color -%>;
        text-decoration: none;
    }

    #headline p {
        color: <%= @studio.minisite.font_color -%>;
        font-family: Georgia, Times New Roman, Times, serif;
        font-size: 18px;
        text-align: center;
        margin-top: 0px;
        margin-bottom: 30px;
    }

    #headline p a {
        color: <%= @studio.minisite.font_color -%>;
        text-decoration: none;
    }

    .article-title {
        font-size: 18px;
        line-height: 24px;
        color: <%= @studio.minisite.font_color -%>;
        font-weight: bold;
        margin-top: 0px;
        margin-bottom: 18px;
        font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
    }

    .article-title a {
        color: <%= @studio.minisite.font_color -%>;
        text-decoration: none;
    }

    .price {
        font-size: 15px;
        line-height: 18px;
        color: <%= @studio.minisite.font_color -%>;
        margin-top: 0px;
        margin-bottom: 18px;
        font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
    }

    .article-content a {
        color: <%= @studio.minisite.font_color -%>;
        text-decoration: underline;
    }

    .article-content img {
        max-width: 100%
    }

    .article-content ol, .article-content ul {
        margin-top: 0px;
        margin-bottom: 18px;
        margin-left: 19px;
        padding: 0;
    }

    .article-content li {
        font-size: 13px;
        line-height: 18px;
        color: <%= @studio.minisite.font_color -%>;
    }

    .article-content li a {
        color: <%= @studio.minisite.font_color -%>;
        text-decoration: underline;
    }

    .article-content p {
        margin-bottom: 15px;
    }

    .footer-content-left {
        font-size: 12px;
        line-height: 15px;
        color: #e2e2e2;
        margin-top: 0px;
        margin-bottom: 15px;
    }

    .footer-content-left a {
        color: #e2e2e2;
        font-weight: bold;
        text-decoration: none;
    }

    .footer-content-right {
        font-size: 11px;
        line-height: 16px;
        color: #e2e2e2;
        margin-top: 0px;
        margin-bottom: 15px;
    }

    .footer-content-right a {
        color: #e2e2e2;
        font-weight: bold;
        text-decoration: none;
    }

    #footer {
        background-color: #666666;
        color: #e2e2e2;
    }

    #footer a {
        color: #e2e2e2;
        text-decoration: none;
        font-weight: bold;
    }

    #permission-reminder {
        white-space: normal;
    }

    #street-address {
        color: #e2e2e2;
        white-space: normal;

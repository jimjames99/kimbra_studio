<%= f.fields_for :info_studio, @studio.info_studio do |g| %>
    <div class='field'>
      <%= g.label :email, 'Email' %><br/>
      <%= g.email_field :email %>
    </div>
    <div class='field'>
      <%= g.label :email_info, 'Email info' %><br/>
      <%= g.email_field :email_info %>
    </div>
    <div class='field'>
      <%= g.label :website %><br/>
      <%= g.text_field :website %>
    </div>
    <div class='field'>
      <%= g.label :tax_id, 'Tax ID/EIN' %><br/>
      <%= g.text_field :tax_id %>
    </div>
    <div class='field'>
      <%= g.label :active %>
      <%= g.check_box :active %>
    </div>
    <div class='field'>
      <%= g.label :pictage_member %>
      <%= g.check_box :pictage_member %>
    </div>
    <div class='field'>
      <%= g.label :mac_user %>
      <%= g.check_box :mac_user %>
    </div>
    <div class='field'>
      <%= g.label :windows_user %>
      <%= g.check_box :windows_user %>
    </div>
    <div class='field'>
      <%= g.label :ping_email, 'Notification emails' %>
      <%= g.check_box :ping_email %>
    </div>
<% end %>

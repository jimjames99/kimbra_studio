<div class='row'>
  <div class='span12'>
    <%= render 'my_studio/dashboards/sessions', sessions: @sessions %>
  </div>
</div>
<div class='row'>
  <div class='span12'>
    <%= render 'my_studio/dashboards/emails', sessions: @sessions %>
  </div>
</div>
<div class='row'>
  <div class='span12'>
    <%= render 'my_studio/dashboards/ready', sessions: @sessions, emails: @emails %>
  </div>
</div>

<%= render partial: 'shared/unsubscribes', locals: {unsubscribes: @unsubscribes} %>

<% if @prospects.present? %>
    <div class='row'>
      <div class='span12'>
        <h3>Prospects who contacted us</h3>
        <% @prospects.each do |prospect| %>
            <li><%= mail_to prospect.email %> on <%= date_short prospect.created_at %></li>
        <% end %>
      </div>
    </div>
<% end %>

<div class='row'>
  <div class='span6'>
    <br>
    <%= link_to image_tag('kimbra_logo.png') %>
  </div>

</div>
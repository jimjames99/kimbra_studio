<table>
  <thead>
  <th colspan="2"> <%= @part.try(:to_image_span) %></th>
  </thead>
  <tbody>
  <tr>
    <td>Kimbra Piece:</td>
    <td><%= cw_div_image_only(@part.piece) %></td>
  </tr>
  <tr>
    <td>Kimbra Part:</td>
    <td><%= cw_div_image_only(@part, :image => :image_part) %></td>
  </tr>
  <% if @part.portrait %>
      <tr>
        <td>Portrait:</td>
        <td><%= cw_div_image(@part.portrait) %></td>
      </tr>
  <% end %>
  <tr>
    <td>Custom Part:</td>
    <td><%= cw_div_image_only(@part) %></td>
  </tr>
  </tbody>
</table>

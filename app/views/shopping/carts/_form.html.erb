<%= form_for(@shopping_cart) do |f| %>

    <%= render 'shared/error_messages', record: @shopping_cart, h2_msg: 'stopped this cart from being saved:'%>

    <div class="field">
      <%= f.label :email %>
      <br/>
      <%= f.text_field :email %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>

<% end %>
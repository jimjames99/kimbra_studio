<div class='container'>

  <div class='page-header'>

    <h4>
      <%= form_tag new_my_studio_session_path, method: :get do %>
          <button type="submit" class="btn btn-success">
            <i class="icon-plus icon-white"></i>
            <%= t('.create_photo_session') %>...
          </button>
      <% end %>
    </h4>


    <h4>
      <%= t('.revenue', purchases: number_to_currency(@sum_purchases), rate: @commission_rate, total: number_to_currency(@total_commissions)) %>
    </h4>

    <div class='container'>
      <%= render 'my_studio/dashboards/sessions', sessions: @sessions %>
    </div>

    <div class='container'>
      <%= render 'my_studio/dashboards/emails', sessions: @sessions %>
    </div>
  </div>
  <div class='span2'>
    <%= image_tag 'kimbra_logo.png' %>
  </div>
</div>
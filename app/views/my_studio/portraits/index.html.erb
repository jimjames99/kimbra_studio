<h1>Listing my_studio_portraits</h1>
<h3> For <%= @my_studio_session.try(:name) %></h3>

<%= link_to 'New Portrait', new_my_studio_session_portrait_path(@my_studio_session) %>

<table>
  <tr>
    <th>Description</th>
    <th>Active?</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @my_studio_portraits.each do |my_studio_portrait| %>
      <tr>
        <td><%= my_studio_portrait.description %></td>
        <td><%= my_studio_portrait.active %></td>
        <td><%= image_tag_list(my_studio_portrait) %></td>
        <td><%= link_to 'Show', my_studio_session_portrait_path(@my_studio_session, my_studio_portrait) %></td>
        <td><%= link_to 'Edit', edit_my_studio_session_portrait_path(@my_studio_session, my_studio_portrait) %></td>
        <td><%= link_to 'Destroy', my_studio_session_portrait_path(@my_studio_session, my_studio_portrait), confirm: 'Are you sure?', method: :delete %></td>
      </tr>
  <% end %>
</table>

<%= link_to 'Back', my_studio_sessions_path %>

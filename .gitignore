/config/amazon_s3.yml
/config/click_face.yml

# Ignore vendor gems
/vendor/ruby/*

# Ignore CarrierWave uploads
/public/uploads/*

# Ignore temp jpg files
face*.jpg

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
